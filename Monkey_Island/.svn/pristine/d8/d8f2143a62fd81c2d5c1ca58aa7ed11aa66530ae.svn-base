package fr.eseo.command;

/**
 * The class CommandInscription of the design pattern Command
 *
 */
public class CommandInscription implements Command{
	
	private Manager action;
	
	/**
	 * Instantiates a new CommandInscritpion
	 * @param action the action
	 */
	public CommandInscription(Manager action){
		this.action = action;
	}
	
	/**
	 * Execute the action
	 */
	public void execute(){
		action.inscription();
	}
}